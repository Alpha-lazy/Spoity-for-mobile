{
    
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html",
     "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ] }
    ]
  }
